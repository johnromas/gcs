<h1 class="page-header">Doctors</h1>

<% @doctors.each do |doctor| %>
  <p><strong>Name</strong><%= doctor.full_name %></p>
  <p><strong>Address</strong><%= doctor.address %></p>
  <p><strong>City</strong><%= doctor.city %></p>
  <p><strong>State</strong><%= doctor.state.name %></p>
  <p><strong>Zip</strong><%= doctor.zip %></p>
  <p><strong>Phone</strong><%= doctor.phone %></p>
  <p><strong>Mobile</strong><%= doctor.mobile %></p>
  <p><strong>Notes</strong><%= doctor.notes %></p>
  <%= link_to 'Show', doctor %>
  <%= link_to 'Edit', edit_doctor_path(doctor) %>
  <%= link_to 'Destroy', doctor, method: :delete, data: { confirm: 'Are you sure?' } %>
<% end %>

<br>

<%= link_to 'New Doctor', new_doctor_path %>
